{
    "manifest_version": 3,
    "name": "LogIn Extension",
    "version": "1.0",
    "description" : "An extension that helps the user login on different websites, and changes the account based on the time",
    "action": {
        "default_popup" : "popup/popup.html"
    },
    "icons": {
        "16":"images/icon.png"
    },
    "background": {
        "service_worker" : "background.js",
        "type" : "module"
    },
    "permissions": [
        "storage"
    ]
}
  